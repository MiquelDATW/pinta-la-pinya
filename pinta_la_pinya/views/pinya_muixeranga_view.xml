        <record id="view_muixeranga_search" model="ir.ui.view">
            <field name="name">pinya.muixeranga.search</field>
            <field name="model">pinya.muixeranga</field>
            <field name="arch" type="xml">
                <search string="Muixerangues">
                    <field name="actuacio_id"/>
                    <field name="plantilla_id"/>
                    <field name="estat"/>
                    <field name="tipus"/>
                    <field name="pisos"/>
                    <field name="cordons"/>
                    <filter string="Esborrany" name="esborrany" domain="[('estat', '=', 'draft')]"/>
                    <filter string="Descarregat" name="descarregat" domain="[('estat', '=', 'desca')]"/>
                    <filter string="Intent" name="intent" domain="[('estat', '=', 'intent')]"/>
                    <filter string="Caigut" name="caigut" domain="[('estat', '=', 'fail')]"/>
                    <separator/>
                    <filter string="Normal" name="normal" domain="[('tipus', '=', 'normal')]"/>
                    <filter string="Desplegada" name="desplegada" domain="[('tipus', '=', 'desplega')]"/>
                    <filter string="Aixecat" name="aixecat" domain="[('tipus', '=', 'aixeca')]"/>
                    <separator/>
                    <filter string="2 pisos" name="pisos_2" domain="[('pisos', '=', '2')]"/>
                    <filter string="3 pisos" name="pisos_3" domain="[('pisos', '=', '3')]"/>
                    <filter string="4 pisos" name="pisos_4" domain="[('pisos', '=', '4')]"/>
                    <filter string="5 pisos" name="pisos_5" domain="[('pisos', '=', '5')]"/>
                    <filter string="6 pisos" name="pisos_6" domain="[('pisos', '=', '6')]"/>
                    <separator/>
                    <filter string="Any anterior" name="previous_year" domain="[('data', '&gt;=', (datetime.date.today() - relativedelta(months=12)).strftime('%Y-01-01')), ('data', '&lt;=', (datetime.date.today() - relativedelta(months=12)).strftime('%Y-12-31'))]"/>
                    <filter string="12 mesos anteriors" name="previous_12_months" domain="[('data', '&gt;=', (datetime.date.today() - relativedelta(months=12)).strftime('%Y-%m-01')), ('data', '&lt;', datetime.date.today().strftime('%Y-%m-01'))]"/>
                    <filter string="Mes anterior" name="previous_month" domain="[('data', '&gt;=', (datetime.date.today() - relativedelta(months=1)).strftime('%Y-%m-01')), ('data', '&lt;', datetime.date.today().strftime('%Y-%m-01'))]"/>
                    <filter string="Any actual" name="current_year" domain="[('data', '&gt;=', datetime.date.today().strftime('%Y-01-01'))]"/>
                    <filter string="Mes actual" name="current_month" domain="[('data', '&gt;=', datetime.date.today().strftime('%Y-%m-01'))]"/>
                    <separator/>
                    <group expand="0" string="Group By">
                        <filter name="group_actuacio" string="Actuació" domain="[]" context="{'group_by':'actuacio_id'}"/>
                        <filter name="group_plantilla" string="Plantilla" domain="[]" context="{'group_by':'plantilla_id'}"/>
                        <filter name="group_estat" string="Estat" domain="[]" context="{'group_by':'estat'}"/>
                        <filter name="group_data" string="Data" domain="[]" context="{'group_by':'data'}"/>
                        <filter name="group_tipus" string="Tipus" domain="[]" context="{'group_by':'tipus'}"/>
                        <filter name="group_pisos" string="Pisos" domain="[]" context="{'group_by':'pisos'}"/>
                    </group>
                </search>
            </field>
        </record>

