        <record id="view_muixeranga_tronc_search" model="ir.ui.view">
            <field name="name">pinya.muixeranga.tronc.search</field>
            <field name="model">pinya.muixeranga.tronc</field>
            <field name="arch" type="xml">
                <search string="Tronc de muixeranga">
                    <field name="actuacio_id"/>
                    <field name="muixeranga_tronc_id"/>
                    <field name="plantilla_id"/>
                    <field name="name"/>
                    <field name="posicio_id"/>
                    <field name="tecnica" widget="priority"/>
                    <field name="membre_tronc_id"/>
                    <separator/>
                    <filter string="No establert" name="no_establert" domain="[('membre_tronc_id', '=', False)]"/>
                    <filter string="Establert" name="establert" domain="[('membre_tronc_id', '!=', False)]"/>
                    <separator/>
                    <filter string="1r pis" name="1r_pis" domain="[('name', '=', '1')]"/>
                    <filter string="2n pis" name="2n_pis" domain="[('name', '=', '2')]"/>
                    <filter string="3r pis" name="3r_pis" domain="[('name', '=', '3')]"/>
                    <filter string="4t pis" name="4t_pis" domain="[('name', '=', '4')]"/>
                    <filter string="5é pis" name="5e_pis" domain="[('name', '=', '5')]"/>
                    <filter string="6é pis" name="6e_pis" domain="[('name', '=', '6')]"/>
                    <separator/>
                    <filter string="Tècnica ⭐⭐⭐" name="tecnica_3" domain="[('tecnica', '=', '3')]"/>
                    <filter string="Tècnica ⭐⭐" name="tecnica_2" domain="[('tecnica', '=', '2')]"/>
                    <filter string="Tècnica ⭐" name="tecnica_1" domain="[('tecnica', '=', '1')]"/>
                    <filter string="Tècnica 🌑" name="tecnica_0" domain="[('tecnica', '=', '0')]"/>
                    <separator/>
                    <filter string="Any anterior" name="previous_year" domain="[('data', '&gt;=', (datetime.date.today() - relativedelta(months=12)).strftime('%Y-01-01')), ('data', '&lt;=', (datetime.date.today() - relativedelta(months=12)).strftime('%Y-12-31'))]"/>
                    <filter string="12 mesos anteriors" name="previous_12_months" domain="[('data', '&gt;=', (datetime.date.today() - relativedelta(months=12)).strftime('%Y-%m-01')), ('data', '&lt;', datetime.date.today().strftime('%Y-%m-01'))]"/>
                    <filter string="Mes anterior" name="previous_month" domain="[('data', '&gt;=', (datetime.date.today() - relativedelta(months=1)).strftime('%Y-%m-01')), ('data', '&lt;', datetime.date.today().strftime('%Y-%m-01'))]"/>
                    <filter string="Any actual" name="current_year" domain="[('data', '&gt;=', datetime.date.today().strftime('%Y-01-01'))]"/>
                    <filter string="Mes actual" name="current_month" domain="[('data', '&gt;=', datetime.date.today().strftime('%Y-%m-01'))]"/>
                    <separator/>
                    <group expand="0" string="Group By">
                        <filter name="group_actuacio" string="Actuació" domain="[]" context="{'group_by':'actuacio_id'}"/>
                        <filter name="group_muixeranga_tronc" string="Figura" domain="[]" context="{'group_by':'muixeranga_tronc_id'}"/>
                        <filter name="group_plantilla" string="Plantilla" domain="[]" context="{'group_by':'plantilla_id'}"/>
                        <filter name="group_data" string="Data" domain="[]" context="{'group_by':'data'}"/>
                        <filter name="group_name" string="Pis" domain="[]" context="{'group_by':'name'}"/>
                        <filter name="group_posicio" string="Posició" domain="[]" context="{'group_by':'posicio_id'}"/>
                        <filter name="group_tecnica" string="Tècnica" domain="[]" context="{'group_by':'tecnica'}"/>
                        <filter name="group_membre_tronc" string="Membre Tronc" domain="[]" context="{'group_by':'membre_tronc_id'}"/>
                    </group>
                </search>
            </field>
        </record>

